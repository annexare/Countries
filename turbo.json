{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "//#lint": {
      "cache": false
    },
    "//#check-types": {
      "cache": false
    },
    "countries#build": {
      "dependsOn": ["//#lint", "//#check-types", "scripts#build"]
    },
    "scripts#build": {
      "cache": false,
      "dependsOn": ["//#lint", "//#check-types"]
    },
    "build": {
      "dependsOn": ["//#lint", "//#check-types"],
      "outputs": ["dist/**"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["//#lint", "//#check-types", "scripts#build", "countries#build"]
    }
  }
}
