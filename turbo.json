{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "countries#build": {
      "dependsOn": [
        "lint",
        "scripts#build"
      ]
    },
    "scripts#build": {
      "dependsOn": [
        "lint"
      ]
    },
    "build": {
      "dependsOn": [
        "lint"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "test": {
      "dependsOn": [
        "lint",
        "scripts#build",
        "countries#build"
      ]
    }
  }
}
